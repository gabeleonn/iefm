{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@next/eslint-plugin-next", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
  }
}
